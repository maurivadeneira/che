{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/images/fondos/(.*)",
      "dest": "/images/fondos/$1"
    },
    {
      "src": "/fondos/(.*)",
      "dest": "/fondos/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/documentos/(.*)",
      "dest": "/documentos/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/$1.png"
    },
    {
      "src": "/(.*)\\.svg",
      "dest": "/$1.svg"
    },
    {
      "src": "/(.*)\\.jpg",
      "dest": "/$1.jpg"
    },
    {
      "src": "/(.*)\\.jpeg",
      "dest": "/$1.jpeg"
    },
    {
      "src": "/(.*)\\.gif",
      "dest": "/$1.gif"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}