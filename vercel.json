{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/documentos/(.*\\.pdf)",
      "dest": "/documentos/$1"
    },
    {
      "src": "/documentos/articulos/(.*\\.pdf)",
      "dest": "/documentos/articulos/$1"
    },
    {
      "src": "/documentos/libros/(.*\\.pdf)",
      "dest": "/documentos/libros/$1"
    },
    {
      "src": "/documentos/herejias-ia/(.*\\.pdf)",
      "dest": "/documentos/herejias-ia/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}