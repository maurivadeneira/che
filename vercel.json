{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "devCommand": "npm run dev",
  "headers": [
    {
      "source": "**/*.pdf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/Guia-Familiar-Sistema-Kit2.pdf",
      "destination": "/Guia-Familiar-Sistema-Kit2.pdf"
    },
    { 
      "source": "/((?!.*\\.pdf$).*)", 
      "destination": "/" 
    }
  ]
}