{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/documentos/(.*)",
      "dest": "/documentos/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
