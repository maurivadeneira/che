{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "headers": [
    {
      "source": "/documentos/articulos/(.*\\.pdf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/documentos/articulos/(.*)",
      "destination": "/documentos/articulos/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}